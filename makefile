all:
	sudo apt-get update							# Atualizar os repositórios do ubuntu
	sudo apt-get install python3-pip            # Instalar o pip
	pip3 --version                              # Verifica a versão do pip
	sudo apt-get install python3-numpy          # Instala a biblioteca do numpy
	sudo apt-get install python3-matplotlib     # Instala a biblioteca do matplotlib   



